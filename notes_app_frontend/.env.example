# Environment configuration for Notes App (React)
# Note: Do not commit real secrets. Copy this to .env and edit as needed.

# If set, the app may use an API backend. If empty, localStorage is used.
REACT_APP_API_BASE=

# Optional additional endpoints
REACT_APP_BACKEND_URL=
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=

# Runtime behavior
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=1
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=0
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/healthz
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=0
